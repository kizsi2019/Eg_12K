        <CheckBox Name="kevesCB" Content="Nem megfelelő a karakterek száma ()" HorizontalAlignment="Left" Height="23" Margin="42,154,0,0" VerticalAlignment="Top" Width="244"/>
        <CheckBox Name="helytelenCB" Content="Helytelen karakter az eredményekben ()" HorizontalAlignment="Left" Height="23" Margin="42,182,0,0" VerticalAlignment="Top" Width="244"/>
        <Button Name="mentes" Content="Eredmények mentése" HorizontalAlignment="Left" Height="23" Margin="42,243,0,0" VerticalAlignment="Top" Width="203" />

    </Grid>
</Window>
